{
  "name": "deflect",
  "description": "functional composition of HTTP servers",
  "version": "0.0.0",
  "main": "deflect.js",
  "devDependencies": {
    "chai": "^1.10.0",
    "mocha": "^2.1.0",
    "semver": "^4.2.0",
    "supertest": "^0.15.0"
  },
  "scripts": {
